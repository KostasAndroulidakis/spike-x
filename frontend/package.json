{
  "name": "frontend",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "services:start": "brew services start postgresql@15 && brew services start redis",
    "services:stop": "brew services stop postgresql@15 && brew services stop redis",
    "dev:full": "npm run services:start && npm run dev"
  },
  "dependencies": {
    "@react-spring/web": "^9.7.5",
    "@react-three/drei": "^9.120.5",
    "@react-three/fiber": "^8.17.10",
    "@remix-run/node": "^2.15.2",
    "@remix-run/react": "^2.15.2",
    "@remix-run/serve": "^2.15.2",
    "chart.js": "^4.4.7",
    "google-protobuf": "^3.21.4",
    "grpc-web": "^1.5.0",
    "isbot": "^4.1.0",
    "lucide-react": "^0.469.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.4.0",
    "react-toggle": "^4.1.3",
    "three": "^0.172.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.15.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
